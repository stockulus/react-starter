{
  "name": "react-starter",
  "version": "0.1.0",
  "description": "React starter Project",
  "scripts": {
    "start": "npm run develop",
    "test": "node ./node_modules/standard/bin/cmd.js && ./node_modules/babel-cli/bin/babel-node.js ./node_modules/tape/bin/tape ./source/tests/**/*.js | ./node_modules/faucet/bin/cmd.js",
    "watch-test": "node ./node_modules/nodemon/bin/nodemon.js -e js,jsx --watch ./source/ --exec 'npm run test'",
    "develop": "node ./node_modules/gulp/bin/gulp.js",
    "build": "node ./node_modules/gulp/bin/gulp.js build"
  },
  "keywords": [
    "starter",
    "react",
    "gulp",
    "tape"
  ],
  "author": "Christoph Stock (@stockulus)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babelify": "7.3.0",
    "browser-sync": "2.12.5",
    "browserify": "13.0.0",
    "faucet": "0.0.1",
    "gulp": "3.9.1",
    "gulp-clean": "0.3.2",
    "gulp-sourcemaps": "2.0.0-alpha",
    "gulp-util": "3.0.7",
    "jsdom": "8.5.0",
    "lodash.assign": "4.0.8",
    "nodemon": "1.9.2",
    "run-sequence": "1.1.5",
    "standard": "7.0.0",
    "tape": "4.5.1",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0",
    "watchify": "3.7.0"
  },
  "dependencies": {
    "react": "15.0.2",
    "react-addons-test-utils": "15.0.2",
    "react-dom": "15.0.2",
    "redux": "3.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stockulus/react_starter"
  }
}

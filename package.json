{
  "name": "react-starter",
  "version": "0.1.0",
  "description": "React starter Project",
  "scripts": {
    "start": "npm run develop",
    "test": "node ./node_modules/standard/bin/cmd.js && ./node_modules/babel-cli/bin/babel-node.js ./node_modules/tape/bin/tape ./source/tests/**/*.js | ./node_modules/faucet/bin/cmd.js",
    "watch-test": "node ./node_modules/nodemon/bin/nodemon.js -e js,jsx --watch ./source/ --exec 'npm run test'",
    "develop": "node ./node_modules/gulp/bin/gulp.js",
    "build": "node ./node_modules/gulp/bin/gulp.js build"
  },
  "keywords": [
    "starter",
    "react",
    "gulp",
    "tape"
  ],
  "author": "Christoph Stock (@stockulus)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.1.2",
    "babel-preset-es2015": "6.0.15",
    "babel-preset-react": "6.0.15",
    "babelify": "7.2.0",
    "browser-sync": "2.9.11",
    "browserify": "12.0.1",
    "faucet": "0.0.1",
    "gulp": "3.9.0",
    "gulp-clean": "0.3.1",
    "gulp-run-sequence": "0.3.2",
    "gulp-sourcemaps": "1.6.0",
    "gulp-util": "3.0.7",
    "jsdom": "^7.0.2",
    "lodash.assign": "3.2.0",
    "nodemon": "1.8.1",
    "standard": "5.3.1",
    "tape": "4.2.2",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0",
    "watchify": "3.6.0"
  },
  "dependencies": {
    "react": "0.14.2",
    "react-addons-test-utils": "0.14.2",
    "react-dom": "0.14.2",
    "redux": "3.0.4"
  },
  "repository" :
  {
     "type": "git",
     "url": "https://github.com/stockulus/react_starter"
  }
}
